<% include header_min %>
  <div class="container">
    <h3 class="title">友情链接列表</h3>
    <table class="table listuser-table">
      <tr>
        <td>友情链接名称</td>
        <td>地址</td>
        <td>时间</td>
        <td>操作</td>
      </tr>
      <% datalist.forEach(function(val){ %>
      <tr>
        <td class="a"><%=val.name%></td>
        <td class="a"><%=val.address%></td>
        <td class="a"><%=val.ctime.toLocaleString()%></td>
        <td class="a">
          <a href="/admin/freEdit/<%=val._id%>">编辑</a>
          |
          <a href="/admin/freDel/<%=val._id%>" onclick="return confirm('你确定要删除吗？')">删除</a>
        </td> 
      </tr>
      <% }) %>
    </table>
  </div>
<% include footer_min %>